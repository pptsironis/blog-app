<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="postTitleTitle">Title</label>
        <input class="form-control" type="text" name="postTitle" id="postTitleTitle">
    </div>
    <div class="form-group">
        <label for="postContentTitle">Post</label>
        <textarea class="form-control" name="postContent" id="postContentTitle" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>